package com.infotech.model;

import com.infotech.model.*;

public class Triangle 
{
	private Point pointA;

	private Point pointB;
	
	private Point pointC;
	
	public Point getPointA() {
		return pointA;
	}

	public void setPointA(Triangle pointA) {
		this.pointA = pointA;
	}

	public Point getPointB() {
		return pointB;
	}

	public void setPointB(Triangle pointB) {
		this.pointB = pointB;
	}

	public Triangle getPointC() {
		return pointC;
	}

	public void setPointC(Triangle pointC) {
		this.pointC = pointC;
	}
	
public void draw() {
	System.out.println("Point A =" +getPointA().getX());
}

}


